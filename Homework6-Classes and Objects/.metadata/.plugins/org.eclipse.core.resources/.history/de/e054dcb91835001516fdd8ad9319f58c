package number_1;

public class Cat {
	private String name;
	private int years;
	
	public Cat(){
		this.name="";
		this.years=0;
	}
	public Cat(String Name, int Years){
		this();
		setName(Name);
		setYears(Years);
	}
	public String getName(){
		return name;
	}
	public void setName(String Name){
		this.name=Name;
		
	}
	public int getYears(){
		return years;
	}
	public void setYears(int Years){
		this.years=Years;
		
	}
	public void Say(){
		System.out.println("My name is: "+this.name+". I am "+this.years+" years ols");
	}

	public static void main(String[] args) {
		Cat NewCat= new Cat("Rijko",2);
		NewCat.Say();

	}

}
