package number_3;

public class Desserts extends Stock {
	private int calories;
	private String Name;
	
	public Desserts()
	{
		super();
		this.calories=0;
		this.Name="";
	}
	public Desserts(double price, Boolean InStock,int calories, String Name){
		super(price,InStock);
		setCalories(calories);
		setName(Name);
	}
	public int getCalories() {
		return calories;
	}
	public void setCalories(int calories) {
		this.calories = calories;
	}
	public String getName() {
		return Name;
	}
	public void setName(String name) {
		Name = name;
	}
	
}
