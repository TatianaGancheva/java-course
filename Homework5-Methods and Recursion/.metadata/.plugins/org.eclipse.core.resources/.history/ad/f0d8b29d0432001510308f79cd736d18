package methods_And_Arrays;

public class Number_5 {

	public static void main(String[] args) {
		int[] arr= new int[989];
		for(int j=0;j<arr.length; j++){
			for(int i=10;i<999;i++){
				arr[j]=i;
			}
		
		}
		int a=arr.length;
		System.out.println(a);
		for(int k: arr){
			System.out.print(k+ " ");
		}
		

	}

}
