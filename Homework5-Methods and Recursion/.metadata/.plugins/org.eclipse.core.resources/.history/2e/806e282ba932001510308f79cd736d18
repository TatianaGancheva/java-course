package methods_And_Arrays;

public class Number_5 {

	public static void main(String[] args) {
		int[] arr= new int[989];
		//int reverseNum=0;
		for(int j=0;j<arr.length; j++){
				arr[j]=j+10;
				
				
			}
//		for(int k: arr){
//			System.out.println(k+ " ");
//		}
		System.out.println("a"+arr[5]);
		System.out.println("The symetric numbers are: ");
		for(int j=0;j<arr.length;j++){
			int k= arr[j];
			int temp;
			boolean symetric=true;
			while(k>10){
				temp=k%10;
				k/=10;
				if(temp!=(k%10)){
					symetric= false;
				}
				
			}
			if(symetric==true){
				System.out.println(k);
			}
			//System.out.println(reverseNum);
//			if(arr[j]==reverseNum){
//				System.out.println(arr[j]);
			}
		}
		
		}
					
	//}
	


