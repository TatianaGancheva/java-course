import java.util.Scanner;
public class Number_9_Backwards {

	public static void main(String[] args) {
		Scanner input= new Scanner(System.in, "UTF-8");
		System.out.println("Enter how long will be the array: ");
		int k= input.nextInt();
		int [] Array= new int[k];
		//int [] reverseArray= new int [k];
		System.out.println("Please enter your numbers now: ");
		for(int i=0; i<k;i++){
			Array[i]=input.nextInt();
		}
//		System.out.println("The array backwards is: ");
//		for(int j=0; j<k; j--){
//			reverseArray[k-j-1]=Array[j];
//			
//		}
		for( int arr : Array){
			System.out.print(arr+ " ");
		}
		input.close();

	}

}
