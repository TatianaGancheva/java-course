import java.util.Scanner;
public class Number_4 {

	public static void main(String[] args) {
		Scanner input=new Scanner(System.in,"UTF-8");
		System.out.println("Enter N: ");
		int N=input.nextInt();
		System.out.println("Enter K: ");
		int K= input.nextInt();
		//int N1;
		//int N2;
		
		for(int i=1; i<N; i++){
			N*=i;
		}
		System.out.println(N);
		input.close();
	}

}
