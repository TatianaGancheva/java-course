import java.util.Scanner;
public class Number_4 {

	public static void main(String[] args) {
		Scanner input=new Scanner(System.in,"UTF-8");
		System.out.println("Enter N: ");
		int N=input.nextInt();
		System.out.println("Enter K: ");
		int K= input.nextInt();
		int result=1;
		//int N2;
		
		for(int i=1; i<=N; i++){
			result=result*i;
		}
		System.out.println(result);
		input.close();
	}

}
